{
  "name": "tasdan/column-searchable",
  "description": "Package for searching by columns in Laravel",
  "keywords": [
    "searchable",
    "sorting",
    "column",
    "search",
    "laravel"
  ],
  "license": "MIT",
  "type": "package",
  "authors": [
    {
      "name": "Daniel Tasi",
      "email": "daniel.tasi@smallthings.hu",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0|^8.0",
    "illuminate/support": "5.8.*|^6.0|^7.0|^8.0",
    "illuminate/database": "5.8.*|^6.0|^7.0|^8.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "Tasdan\\ColumnSearchable\\": "src/ColumnSearchable/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tasdan\\ColumnSearchable\\ColumnSearchableServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
